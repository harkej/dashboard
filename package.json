    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.0.15",
    "css-loader": "^0.26.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.3.0",
